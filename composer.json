{
    "name": "alangiacomin/laravel-cqrs",
    "description": "Laravel extension for CQRS",
    "keywords": [
        "alangiacomin",
        "cqrs",
        "laravel",
        "laravel-cqrs"
    ],
    "homepage": "https://github.com/alangiacomin/laravel-cqrs",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Giacomin",
            "email": "alan.giacomin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "alangiacomin/php-utils": "^0.0.1",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "illuminate/support": "^8.70",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Alangiacomin\\LaravelCqrs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alangiacomin\\LaravelCqrs\\LaravelCqrsServiceProvider",
                "Alangiacomin\\LaravelCqrs\\Providers\\EventServiceProvider"
            ],
            "aliases": {
                "LaravelCqrs": "Alangiacomin\\LaravelCqrs\\LaravelCqrsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
